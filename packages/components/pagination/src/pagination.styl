.sw-pagination
  display flex
  align-items center
  justify-content flex-start
  &-total
    font-size 13px
    height 28px
    line-height 28px
    font-weight 400
    color #606266
  &-page
    display flex
    align-items center
    justify-content center
    &-item
      font-size 13px
      min-width 30px
      height 28px
      line-height 28px
      text-align center
      font-size 13px
      color #303133
      font-weight 700
      white-space nowrap
      &-icon
        line-height 28px
      &:hover
        color #0070d2
    .active
      color #0070d2
  &-goto
    display flex
    align-items center 
    .sw-input
      text-align center
      
      
